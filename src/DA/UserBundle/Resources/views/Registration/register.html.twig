{% extends "DACoreBundle::layout.html.twig" %}

{% block body %}
    <h3>Formulaire d'inscription</h3>
    <div class="well">

        {{ form_start(form) }}
            {{ form_row(form.username) }}
            {{ form_row(form.email) }}
            {{ form_row(form.plainPassword.first) }}
            {{ form_row(form.plainPassword.second) }}

        <button type="submit">Register!</button>
        {{ form_end(form) }}
    </div>
{% endblock %}